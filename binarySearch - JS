function binSearch(inputArr = [1,2,3,4,5], key=2) {
        var start = 0;
        var end = inputArr.length - 1;
        while (start <= end) {
            var mid = Math.ceil((start + end) / 2);
            if (key == inputArr[mid]) {
                return mid;
            }
            if (key < inputArr[mid]) {
                end = mid - 1;
            } else {
                start = mid + 1;
            }
        }
        return -1;
    }
